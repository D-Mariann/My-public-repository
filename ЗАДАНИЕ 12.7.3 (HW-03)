per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
percent1 = list(per_cent.values())
print(percent1)
money = int(input('Введите сумму:'))
d1 = percent1[0]*money/100
d2 = percent1[1]*money/100
d3 = percent1[2]*money/100
d4 = percent1[3]*money/100
deposit = [d1,d2,d3,d4]
print(deposit)
deposit.sort()
print('Максимальная сумма, которую вы можете заработать —', deposit[-1])
